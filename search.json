---
---
[
  {% for post in site.posts %}
  {

    "title"    : "{{ post.title | escape }}",
    "url"      : "/dev_site2{{ post.url }}",
    "date"     : "{{ post.date }}",
    "content" : "{{ post.content | strip_html | strip_newlines | truncatewords: 400, "" | rstrip | lstrip | escape }}"


  } {% unless forloop.last %},{% endunless %}
  {% endfor %}

]

